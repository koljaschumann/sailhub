{
  "name": "tsc-jugend-plattform",
  "version": "1.0.0",
  "private": true,
  "description": "TSC Jugend Plattform - Unified Platform for Tegeler Segel-Club",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "dev:web": "turbo run dev --filter=web",
